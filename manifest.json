{
  "manifest_version": 2,
  "name": "Clean Linkedin",
  "version": "1.0.1",
  "description": "Linkedin Clear Bullshit",
  "icons": {
    "16": "img/icons16.png"
  },
  "browser_action": {
    "default_icon": "img/icons16.png",
    "default_popup": "popup.html",
    "default_title": "Linkedin with Less BS"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/feed/"],
      "js": ["/content_script.js"]
    }
  ],
  "permissions": [ "tabs", "activeTab"]
}
